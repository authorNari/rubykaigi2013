%h3<>= slot
- ['A', 'B'].each_with_index do |hall, index|
  - next if cells[index].blank?

  %ul<>
    %h4= hall
    - cells[index].each do |hash|
      - type, id = hash.first

      %li
        - if type == :sessions
          - session = data.sessions[id.to_s]
          = link_to "'#{session.title}'", "/2013/session/#{id}", class: 'sessionTitle'
          \-
          = session.speakers.map(&:name).join(', ')

        - elsif type == :events
          - event = data.events[id.to_s]
          %span.event= event.title
          - if event.organizer.present?
            \-
            = event.organizer.map(&:name).join(', ')

        - elsif type == :keynotes
          - keynote = data.keynotes[id.to_s]
          %span.keynote Keynote
          \-
          = keynote.speaker.name

        - else
          (missing id: #{type}:#{id})
